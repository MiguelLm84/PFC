<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/FrameLayoutGestion"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".ui.FragmentGestion">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageButton
            android:id="@+id/bt_volver_gestion"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="40dp"
            android:layout_marginTop="32dp"
            android:background="@null"
            android:contentDescription="@string/todo"
            android:src="@drawable/flecha"
            android:tint="@color/black"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/view_gestion"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:background="@color/primary"
            android:visibility="gone"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/view_inferior_getion"
            android:layout_width="wrap_content"
            android:layout_height="90dp"
            android:layout_marginTop="32dp"
            android:background="@drawable/wave"
            android:visibility="gone"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/tv_gestion" />

        <ImageView
            android:id="@+id/imageView_gestion"
            android:layout_width="83dp"
            android:layout_height="68dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="32dp"
            android:contentDescription="@string/todo"
            android:visibility="gone"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/user_round"
            tools:ignore="MissingConstraints" />

        <androidx.cardview.widget.CardView
            android:id="@+id/cardViewLogo"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginTop="24dp"
            android:layout_marginEnd="32dp"
            android:contentDescription="@string/nav_header_desc"
            android:paddingTop="@dimen/nav_header_vertical_spacing"
            android:visibility="gone"
            app:cardBackgroundColor="#FFFFFF"
            app:contentPadding="1dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.cardview.widget.CardView
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:background="@drawable/logo"
                android:foreground="@drawable/logo"
                app:cardBackgroundColor="@android:color/transparent" />
        </androidx.cardview.widget.CardView>

        <TextView
            android:id="@+id/tv_gestion"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="112dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="40dp"
            android:fontFamily="@font/amaranth"
            android:padding="10dp"
            android:text="@string/area_gestion"
            android:textAlignment="center"
            android:textAllCaps="false"
            android:textColor="#FFFFFF"
            android:textSize="20sp"
            android:textStyle="bold"
            android:visibility="gone"
            app:layout_constraintEnd_toStartOf="@+id/cardViewLogo"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/view_gestion" />

        <ImageButton
            android:id="@+id/btn_personalizacion_app"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginTop="32dp"
            android:background="@null"
            android:backgroundTint="@android:color/transparent"
            android:contentDescription="@string/todo"
            app:layout_constrainedHeight="false"
            app:layout_constrainedWidth="false"
            app:layout_constraintStart_toEndOf="@+id/bt_volver_gestion"
            app:layout_constraintTop_toBottomOf="@+id/view_inferior_getion"
            app:srcCompat="@drawable/settings" />

        <ImageButton
            android:id="@+id/btn_estadisticas"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="29dp"
            android:layout_marginEnd="16dp"
            android:background="@null"
            android:backgroundTint="@android:color/transparent"
            android:contentDescription="@string/todo"
            android:visibility="gone"
            app:layout_constrainedHeight="false"
            app:layout_constrainedWidth="false"
            app:layout_constraintEnd_toStartOf="@+id/btn_notificacion_individual"
            app:layout_constraintTop_toBottomOf="@+id/view_inferior_getion"
            app:srcCompat="@drawable/estadisticas__2_" />

        <ImageButton
            android:id="@+id/btn_recordatorios"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="28dp"
            android:layout_marginEnd="16dp"
            android:background="@null"
            android:backgroundTint="@android:color/transparent"
            android:contentDescription="@string/todo"
            app:layout_constrainedHeight="false"
            app:layout_constrainedWidth="false"
            app:layout_constraintEnd_toStartOf="@+id/btn_campanhas"
            app:layout_constraintTop_toBottomOf="@+id/view_inferior_getion"
            app:srcCompat="@drawable/ic_baseline_message_24" />

        <ImageButton
            android:id="@+id/btn_notificacion_individual"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="16dp"
            android:background="@null"
            android:backgroundTint="@android:color/transparent"
            android:contentDescription="@string/todo"
            app:layout_constrainedHeight="false"
            app:layout_constrainedWidth="false"
            app:layout_constraintEnd_toStartOf="@+id/btn_recordatorios"
            app:layout_constraintTop_toBottomOf="@+id/view_inferior_getion"
            app:srcCompat="@drawable/notificacion" />

        <ImageButton
            android:id="@+id/btn_campanhas"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="24dp"
            android:layout_marginEnd="40dp"
            android:background="@null"
            android:clickable="true"
            android:contentDescription="@string/todo"
            android:focusable="true"
            android:src="@drawable/correo_de_propaganda"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view_inferior_getion" />

        <ImageButton
            android:id="@+id/btn_junta_directiva"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="26dp"
            android:layout_marginEnd="16dp"
            android:background="@null"
            android:contentDescription="@string/todo"
            android:src="@drawable/administrador_de_grupo"
            app:layout_constraintEnd_toStartOf="@+id/btn_notificacion_individual"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/tv_num_socio3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="90dp"
            android:layout_marginTop="24dp"
            android:autofillHints=""
            android:background="@drawable/round_border"
            android:drawableStart="@drawable/ic_baseline_format_list_numbered_24"
            android:drawablePadding="16dp"
            android:focusable="false"
            android:focusableInTouchMode="false"
            android:fontFamily="@font/amaranth"
            android:gravity="center"
            android:inputType="number"
            android:padding="10dp"
            android:textAlignment="textStart"
            android:textColor="@color/primary"
            android:textSize="16sp"
            android:visibility="gone"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view_inferior_getion"
            tools:ignore="LabelFor,RtlCompat" />

        <EditText
            android:id="@+id/tv_nom3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginTop="90dp"
            android:layout_marginEnd="32dp"
            android:autofillHints=""
            android:background="@drawable/round_border"
            android:drawableStart="@drawable/ic_baseline_person"
            android:drawablePadding="16dp"
            android:focusable="false"
            android:focusableInTouchMode="false"
            android:fontFamily="@font/amaranth"
            android:gravity="center"
            android:inputType="textPersonName"
            android:padding="10dp"
            android:textAlignment="textStart"
            android:textColor="@color/primary"
            android:textSize="16sp"
            android:visibility="gone"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view_inferior_getion"
            tools:ignore="LabelFor,RtlCompat" />

        <EditText
            android:id="@+id/tv_ap4"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="32dp"
            android:autofillHints=""
            android:background="@drawable/round_border"
            android:drawableStart="@drawable/ic_baseline_person"
            android:drawablePadding="16dp"
            android:focusable="false"
            android:focusableInTouchMode="false"
            android:fontFamily="@font/amaranth"
            android:gravity="center"
            android:inputType="textPersonName"
            android:padding="10dp"
            android:textAlignment="textStart"
            android:textColor="@color/primary"
            android:textSize="16sp"
            android:visibility="gone"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_nom3"
            tools:ignore="LabelFor,RtlCompat" />

        <EditText
            android:id="@+id/tv_ap5"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="32dp"
            android:autofillHints=""
            android:background="@drawable/round_border"
            android:drawableStart="@drawable/ic_baseline_person"
            android:drawablePadding="16dp"
            android:focusable="false"
            android:focusableInTouchMode="false"
            android:fontFamily="@font/amaranth"
            android:gravity="center"
            android:inputType="textPersonName"
            android:padding="10dp"
            android:textAlignment="textStart"
            android:textColor="@color/primary"
            android:textSize="16sp"
            android:visibility="gone"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_ap4"
            tools:ignore="LabelFor,RtlCompat" />

        <EditText
            android:id="@+id/tv_fechaNaci3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="32dp"
            android:autofillHints=""
            android:background="@drawable/round_border"
            android:drawableStart="@drawable/ic_sharp_date_range_24"
            android:drawablePadding="16dp"
            android:focusable="false"
            android:focusableInTouchMode="false"
            android:fontFamily="@font/amaranth"
            android:gravity="center"
            android:inputType="date"
            android:padding="10dp"
            android:textAlignment="textStart"
            android:textColor="@color/primary"
            android:textSize="16sp"
            android:visibility="gone"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_ap5"
            tools:ignore="LabelFor,RtlCompat" />

        <EditText
            android:id="@+id/tv_telefono3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="32dp"
            android:autofillHints=""
            android:background="@drawable/round_border"
            android:drawableStart="@drawable/ic_baseline_phone_24"
            android:drawablePadding="16dp"
            android:focusable="false"
            android:focusableInTouchMode="true"
            android:fontFamily="@font/amaranth"
            android:foregroundGravity="center"
            android:inputType="phone"
            android:padding="10dp"
            android:textAlignment="textStart"
            android:textColor="@color/primary"
            android:textSize="16sp"
            android:visibility="gone"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_fechaNaci3"
            tools:ignore="LabelFor" />

        <EditText
            android:id="@+id/tv_email_perfil3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="32dp"
            android:autofillHints=""
            android:background="@drawable/round_border"
            android:drawableStart="@drawable/ic_baseline_email_24"
            android:drawablePadding="16dp"
            android:focusable="false"
            android:fontFamily="@font/amaranth"
            android:gravity="center"
            android:inputType="textEmailAddress"
            android:padding="10dp"
            android:textAlignment="textStart"
            android:textColor="@color/primary"
            android:textSize="16sp"
            android:visibility="gone"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_telefono3"
            tools:ignore="LabelFor,RtlCompat" />

        <EditText
            android:id="@+id/tv_password_perfil3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="32dp"
            android:autofillHints=""
            android:background="@drawable/round_border"
            android:drawablePadding="16dp"
            android:focusable="false"
            android:fontFamily="@font/amaranth"
            android:gravity="center"
            android:inputType="textPassword"
            android:padding="10dp"
            android:textAlignment="textStart"
            android:textColor="@color/primary"
            android:textSize="16sp"
            android:visibility="gone"
            app:drawableStartCompat="@drawable/ic_baseline_lock_24"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_email_perfil3"
            tools:ignore="LabelFor,RtlCompat" />

        <Button
            android:id="@+id/btn_aceptar3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="end"
            android:layout_marginStart="32dp"
            android:layout_marginTop="32dp"
            android:layout_marginEnd="32dp"
            android:layout_marginBottom="50dp"
            android:background="@drawable/round_background"
            android:backgroundTint="@color/primary"
            android:fontFamily="@font/amaranth"
            android:text="@string/bt_aceptar"
            android:textColor="#FFFFFF"
            android:textSize="15sp"
            android:textStyle="bold"
            android:visibility="gone"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_password_perfil3" />

        <Button
            android:id="@+id/btn_guardar3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="end"
            android:layout_marginStart="32dp"
            android:layout_marginTop="32dp"
            android:layout_marginEnd="32dp"
            android:layout_marginBottom="50dp"
            android:background="@drawable/round_background"
            android:backgroundTint="@color/primary"
            android:fontFamily="@font/amaranth"
            android:text="@string/bt_guardar"
            android:textColor="#FFFFFF"
            android:textSize="15sp"
            android:textStyle="bold"
            android:visibility="gone"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_password_perfil3" />

        <ImageView
            android:id="@+id/bt_volver3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginStart="32dp"
            android:layout_marginTop="45dp"
            android:background="@null"
            android:contentDescription="@string/todo"
            android:visibility="gone"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/flecha" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>