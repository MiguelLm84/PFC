<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.ui.perfil.PerfilFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <EditText
            android:id="@+id/tv_numero_socio"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginTop="70dp"
            android:layout_marginEnd="32dp"
            android:autofillHints=""
            android:background="@drawable/round_border"
            android:drawableStart="@drawable/ic_baseline_format_list_numbered_24"
            android:drawablePadding="16dp"
            android:focusable="false"
            android:focusableInTouchMode="false"
            android:fontFamily="@font/amaranth"
            android:gravity="center"
            android:inputType="number"
            android:padding="10dp"
            android:textAlignment="textStart"
            android:textColor="?attr/colorPrimary"
            android:textSize="16sp"
            app:layout_constraintEnd_toStartOf="@+id/bt_editar"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="LabelFor,RtlCompat" />

        <ImageButton
            android:id="@+id/bt_editar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="40dp"
            android:layout_marginBottom="20dp"
            android:background="@null"
            android:backgroundTint="@android:color/transparent"
            android:contentDescription="@string/todo"
            app:layout_constrainedHeight="false"
            app:layout_constrainedWidth="false"
            app:layout_constraintBottom_toTopOf="@+id/tv_nombre"
            app:layout_constraintEnd_toEndOf="parent"
            app:srcCompat="@drawable/edit_button" />

        <ImageView
            android:id="@+id/bt_eliminar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="40dp"
            android:layout_marginBottom="20dp"
            android:clickable="true"
            android:contentDescription="@string/todo"
            android:focusable="true"
            android:visibility="gone"
            app:layout_constraintBottom_toTopOf="@+id/tv_nombre"
            app:layout_constraintEnd_toEndOf="parent"
            app:srcCompat="@drawable/eliminar__1_" />

        <EditText
            android:id="@+id/tv_nombre"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="32dp"
            android:autofillHints=""
            android:background="@drawable/round_border"
            android:drawableStart="@drawable/ic_baseline_person"
            android:drawablePadding="16dp"
            android:focusable="false"
            android:focusableInTouchMode="false"
            android:fontFamily="@font/amaranth"
            android:gravity="center"
            android:inputType="textPersonName"
            android:padding="10dp"
            android:textAlignment="textStart"
            android:textColor="?attr/colorPrimary"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_numero_socio"
            tools:ignore="LabelFor,RtlCompat" />

        <EditText
            android:id="@+id/tv_apellido1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="32dp"
            android:autofillHints=""
            android:background="@drawable/round_border"
            android:drawableStart="@drawable/ic_baseline_person"
            android:drawablePadding="16dp"
            android:focusable="false"
            android:focusableInTouchMode="false"
            android:fontFamily="@font/amaranth"
            android:gravity="center"
            android:inputType="textPersonName"
            android:padding="10dp"
            android:textAlignment="textStart"
            android:textColor="?attr/colorPrimary"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_nombre"
            tools:ignore="LabelFor,RtlCompat" />

        <EditText
            android:id="@+id/tv_apellido2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="32dp"
            android:autofillHints=""
            android:background="@drawable/round_border"
            android:drawableStart="@drawable/ic_baseline_person"
            android:drawablePadding="16dp"
            android:focusable="false"
            android:focusableInTouchMode="false"
            android:fontFamily="@font/amaranth"
            android:gravity="center"
            android:inputType="textPersonName"
            android:padding="10dp"
            android:textAlignment="textStart"
            android:textColor="?attr/colorPrimary"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_apellido1"
            tools:ignore="LabelFor,RtlCompat" />

        <EditText
            android:id="@+id/tv_fNaci"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="32dp"
            android:autofillHints=""
            android:background="@drawable/round_border"
            android:drawableStart="@drawable/ic_sharp_date_range_24"
            android:drawablePadding="16dp"
            android:focusable="false"
            android:focusableInTouchMode="false"
            android:fontFamily="@font/amaranth"
            android:gravity="center"
            android:inputType="date"
            android:padding="10dp"
            android:textAlignment="textStart"
            android:textColor="@color/primary"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_apellido2"
            tools:ignore="LabelFor,RtlCompat" />

        <EditText
            android:id="@+id/tv_telef"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="32dp"
            android:autofillHints=""
            android:background="@drawable/round_border"
            android:drawableStart="@drawable/ic_baseline_phone_24"
            android:drawablePadding="16dp"
            android:focusable="false"
            android:focusableInTouchMode="true"
            android:fontFamily="@font/amaranth"
            android:foregroundGravity="center"
            android:inputType="phone"
            android:padding="10dp"
            android:textAlignment="textStart"
            android:textColor="?attr/colorPrimary"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_fNaci"
            tools:ignore="LabelFor" />

        <EditText
            android:id="@+id/tv_correo_perfil"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="32dp"
            android:autofillHints=""
            android:background="@drawable/round_border"
            android:drawableStart="@drawable/ic_baseline_email_24"
            android:drawablePadding="16dp"
            android:focusable="false"
            android:fontFamily="@font/amaranth"
            android:gravity="center"
            android:inputType="textEmailAddress"
            android:padding="10dp"
            android:textAlignment="textStart"
            android:textColor="?attr/colorPrimary"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_telef"
            tools:ignore="LabelFor,RtlCompat" />

        <EditText
            android:id="@+id/tv_psswrd_perfil"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="32dp"
            android:autofillHints=""
            android:background="@drawable/round_border"
            android:drawablePadding="16dp"
            android:focusable="false"
            android:fontFamily="@font/amaranth"
            android:gravity="center"
            android:inputType="textPassword"
            android:padding="10dp"
            android:textAlignment="textStart"
            android:textColor="?attr/colorPrimary"
            android:textSize="16sp"
            app:drawableStartCompat="@drawable/ic_baseline_lock_24"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_correo_perfil"
            tools:ignore="LabelFor,RtlCompat" />

        <Button
            android:id="@+id/bt_aceptar"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="end"
            android:layout_marginStart="32dp"
            android:layout_marginTop="24dp"
            android:layout_marginEnd="32dp"
            android:background="@drawable/round_background"
            android:backgroundTint="@color/primary"
            android:fontFamily="@font/amaranth"
            android:text="@string/bt_aceptar"
            android:textColor="#FFFFFF"
            android:textSize="15sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_psswrd_perfil" />

        <Button
            android:id="@+id/bt_guardar"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="end"
            android:layout_marginStart="32dp"
            android:layout_marginTop="24dp"
            android:layout_marginEnd="32dp"
            android:background="@drawable/round_background"
            android:backgroundTint="@color/primary"
            android:fontFamily="@font/amaranth"
            android:text="@string/bt_guardar"
            android:textColor="#FFFFFF"
            android:textSize="15sp"
            android:textStyle="bold"
            android:visibility="gone"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_psswrd_perfil" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>